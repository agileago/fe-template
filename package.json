{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "uat": "vite build --mode uat",
    "release": "vite build",
    "preview": "vite preview --base /",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "api": "tsg -c ./src/api/tsg.config.ts"
  },
  "dependencies": {
    "@abraham/reflection": "^0.10.0",
    "axios": "0.26.1",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.0",
    "ts-deepmerge": "^2.0.1",
    "ts-essentials": "^9.1.2",
    "tslib": "^2.3.1",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "vue3-oop": "^0.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nexhome/yorkie": "^2.0.8",
    "@types/prettier": "^2.6.0",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue3-oop/plugin-vue-jsx": "^1.4.0",
    "autoprefixer": "^10.4.4",
    "co": "4",
    "commitizen": "^4.2.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "less": "^4.1.2",
    "lint-staged": "^12.3.8",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "sass": "^1.50.1",
    "tailwindcss": "^3.0.24",
    "ts-gear": "^4.7.1",
    "typescript": "^4.6.3",
    "vite": "2.9.5",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mockit": "^1.0.2",
    "vite-tsconfig-paths": "^3.4.1",
    "vite-plugin-checker": "^0.4.6",
    "webpack-aliyun-oss": "^0.5.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
