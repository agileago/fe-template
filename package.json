{
  "name": "starter",
  "version": "0.0.0",
  "description": "vite项目模板",
  "packageManager": "pnpm@9.15.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:uat": "vite --mode uat",
    "build": "vite build",
    "build:uat": "vite build --mode uat",
    "preview": "vite preview",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "api": "tsg",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@abraham/reflection": "^0.13.0",
    "axios": "^1.8.4",
    "es-toolkit": "^1.34.1",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^8.2.0",
    "ts-essentials": "^10.0.4",
    "ts-pattern": "^5.7.0",
    "tslib": "^2.8.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-oop": "^1.0.21",
    "@vueuse/core": "^12.7.0",
    "dayjs": "^1.11.8",
    "pinia": "^2.1.3",
    "rattail": "1.0.19"
  },
  "devDependencies": {
    "simple-git-hooks": "^2.12.1",
    "lint-staged": "^15.5.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "eslint": "^9.24.0",
    "@configurajs/eslint": "^0.1.2",
    "prettier": "^3.5.3",
    "@configurajs/prettier": "^0.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-ali-oss": "^2.1.0",
    "vite-plugin-checker": "^0.9.1",
    "vite-svg-loader": "^5.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "@vitejs/plugin-legacy": "^6.0.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue3-oop/plugin-vue-jsx": "^1.5.0",
    "browserslist-to-esbuild": "^2.1.1",
    "@vue3-oop/ts-gear": "^5.0.4",
    "tailwindcss": "^3.4.17",
    "tailwindcss-rem2px-preset": "^1.0.3",
    "less": "^4.3.0",
    "sass": "^1.86.3",
    "postcss": "^8.5.3",
    "postcss-preset-env": "^10.1.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e $1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
