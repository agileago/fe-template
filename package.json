{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "uat": "vite build --mode uat",
    "release": "vite build",
    "preview": "vite preview --base /",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "api": "tsg -c ./src/api/tsg.config.ts"
  },
  "dependencies": {
    "@abraham/reflection": "^0.10.0",
    "axios": "0.27.2",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.1",
    "ts-deepmerge": "^2.0.4",
    "ts-essentials": "^9.2.0",
    "tslib": "^2.4.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vue3-oop": "^0.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nexhome/yorkie": "^2.0.8",
    "@types/lodash": "^4.14.182",
    "@types/prettier": "^2.6.4",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vue3-oop/plugin-vue-jsx": "^1.4.1",
    "autoprefixer": "^10.4.8",
    "co": "4",
    "commitizen": "^4.2.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.54.1",
    "tailwindcss": "^3.1.7",
    "ts-gear": "^4.9.0",
    "typescript": "^4.7.4",
    "vite": "3.0.4",
    "vite-plugin-checker": "^0.4.9",
    "vite-plugin-html-template": "^1.1.5",
    "vite-plugin-mockit": "^1.0.2",
    "vite-tsconfig-paths": "^3.5.0",
    "webpack-aliyun-oss": "^0.5.8"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
