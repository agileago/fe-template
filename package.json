{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "develop": "pnpm run build --mode develop",
    "preview": "vite preview --base /",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@abraham/reflection": "^0.11.0",
    "axios": "0.27.2",
    "injection-js": "^2.4.0",
    "path-to-regexp": "^6.2.1",
    "ts-essentials": "^9.3.0",
    "tslib": "^2.4.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vue3-oop": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/lodash": "^4.14.186",
    "@types/prettier": "^2.7.1",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vue3-oop/plugin-vue-jsx": "^1.4.2",
    "eslint": "~8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "postcss": "^8.4.18",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.55.0",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.1.8",
    "tailwindcss-rem2px-preset": "^1.0.3",
    "terser": "^5.15.1",
    "ts-gear": "^4.11.4",
    "typescript": "^4.8.4",
    "vite": "3.1.8",
    "vite-plugin-ali-oss": "^1.0.7",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-html-template": "^1.1.5",
    "vite-plugin-mockit": "^1.0.2",
    "vite-tsconfig-paths": "^3.5.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
