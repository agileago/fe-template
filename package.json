{
  "name": "starter",
  "version": "0.0.0",
  "description": "vite项目模板",
  "type": "module",
  "scripts": {
    "api": "tsg",
    "build": "bunx --bun vite build",
    "build:uat": "bunx --bun vite build --mode uat",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "dev": "bunx --bun vite",
    "dev:uat": "bunx --bun vite --mode uat",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "simple-git-hooks",
    "preview": "bunx --bun vite preview",
    "type:check": "tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "bun lint-staged",
    "commit-msg": "bun commitlint -e $1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20250826.1",
    "@vueuse/core": "^13.7.0",
    "axios": "^1.10.0",
    "dayjs": "^1.11.8",
    "es-toolkit": "^1.39.10",
    "go-try": "^3.0.2",
    "pinia": "^3.0.3",
    "rattail": "^1.0.22",
    "ts-essentials": "^10.1.1",
    "ts-pattern": "^5.7.1",
    "tslib": "^2.8.1",
    "vue": "^3.5.20",
    "vue-better-props": "^0.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-unicode-property-regex": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@configurajs/eslint": "^0.1.2",
    "@configurajs/prettier": "^0.1.4",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-vue-jsx": "^5.1.0",
    "@vue3-oop/tailwind-preset": "^2.0.1",
    "@vue3-oop/ts-gear": "^5.0.6",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^9.34.0",
    "less": "^4.4.1",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.3.0",
    "prettier": "^3.6.2",
    "sass": "^1.91.0",
    "simple-git-hooks": "^2.13.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-babel": "^1.3.2"
  }
}
