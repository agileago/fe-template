{
  "name": "starter",
  "version": "0.0.0",
  "description": "vite项目模板",
  "type": "module",
  "scripts": {
    "api": "tsg",
    "build": "vite build",
    "build:uat": "vite build --mode uat",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "dev:uat": "vite --mode uat",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "test": "vitest",
    "type:check": "tsgo --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e $1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "vite": "8.0.0-beta.0"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "es-toolkit": "^1.42.0",
    "go-try": "^3.0.2",
    "pinia": "^3.0.4",
    "rattail": "^1.2.1",
    "ts-essentials": "^10.1.1",
    "ts-pattern": "^5.9.0",
    "tslib": "^2.8.1",
    "vue": "^3.5.25",
    "vue-better-props": "^0.0.4",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-unicode-property-regex": "^7.27.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@configurajs/eslint": "^0.1.2",
    "@configurajs/prettier": "^0.1.4",
    "@typescript/native-preview": "^7.0.0-dev.20251204.1",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue3-oop/tailwind-preset": "^2.0.1",
    "@vue3-oop/ts-gear": "^5.0.6",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^9.39.1",
    "happy-dom": "^20.0.11",
    "less": "^4.4.2",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.4.0",
    "prettier": "^3.7.4",
    "sass": "^1.94.2",
    "simple-git-hooks": "^2.13.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.0",
    "vite-plugin-babel": "^1.3.2",
    "vitest": "^4.0.15"
  }
}
